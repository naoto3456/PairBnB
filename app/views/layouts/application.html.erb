<!DOCTYPE html>
<html>
  <head>
    <title>PairBnB</title>
    <%= csrf_meta_tags %>

    <%= stylesheet_link_tag    'application', media: 'all', 'data-turbolinks-track': 'reload' %>
    <%= javascript_include_tag 'application', 'data-turbolinks-track': 'reload' %>
  </head>

  <body>
 	
 	<% if signed_in? %>
		<%= current_user.email %>

		<%= button_to "Sign out", sign_out_path, method: :delete %>
	<% end %>

    <%= yield %>
  
  </body>
</html>
